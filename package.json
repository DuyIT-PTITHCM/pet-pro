{
	"name": "pet-one",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"make-env-dev": "cp ./environment/env.dev.js ./environment/environment.js; cp .env.dev .env",
		"make-env": "cp ./environment/env.prod.js ./environment/environment.js; cp .env.prod .env",
		"fornt-build": "vite build",
		"dev": "npm run make-env-dev; npm i ; nodemon server.js",
		"build": "npm run make-env; vite build",
		"dev-server": "nodemon server.js",
		"start": "node server.js",
		"production": "npm i && npm run dev-server",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.20.4",
		"@types/express": "^4.17.17",
		"@types/node": "^20.4.5",
		"@types/siema": "^1.4.8",
		"autoprefixer": "^10.4.14",
		"carbon-components-svelte": "^0.79.0",
		"carbon-icons-svelte": "^12.1.0",
		"express": "^4.18.2",
		"postcss": "^8.4.27",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.1",
		"body-parser": "^1.20.2",
		"dotenv": "^16.3.1",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.40.2",
		"iconify-icon": "^1.0.8",
		"js-cookie": "^3.0.5",
		"mysql2": "^3.6.0",
		"nodemon": "^3.0.1",
		"sequelize": "^6.32.1",
		"sequelize-paginate": "^1.1.6",
		"siema": "^1.5.1",
		"slick-carousel": "^1.8.1",
		"sveltekit-i18n": "^2.4.2",
		"tailwind-merge": "^1.14.0"
	}
}
